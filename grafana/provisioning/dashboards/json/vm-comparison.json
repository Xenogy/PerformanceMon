      "pluginVersion": "10.2.2",
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "max by (vm_name, vm_id, node, vcpus, memory_gb, disk_type, tags) (up{job=\"windows-vms\", vm_name=~\"$vm\"})",
